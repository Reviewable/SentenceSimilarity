{
  "name": "sentence-similarity",
  "version": "1.0.9",
  "description": "Sentence similarity algorithm.",
  "main": "index.js",
  "scripts": {
    "test": "node jasmine-runner.js",
    "pretty": "prettier --write --tab-width 2 \"**/*.js\"",
    "precommit": "lint-staged && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jloveric/SentenceSimilarity.git"
  },
  "author": "John Loverich",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jloveric/SentenceSimilarity/issues"
  },
  "homepage": "https://github.com/jloveric/SentenceSimilarity#readme",
  "dependencies": {
    "clone": "^2.1.1",
    "damerau-levenshtein": "^1.0.4",
    "debug": "^2.6.6",
    "helper-clockmaker": "^1.0.0",
    "natural": "^0.6.3"
  },
  "keywords": [
    "sentence",
    "similarity",
    "order",
    "size",
    "score",
    "slot"
  ],
  "devDependencies": {
    "husky": "^3.0.9",
    "jasmine": "^2.6.0",
    "jasmine-spec-reporter": "^4.1.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test",
      "...": "..."
    }
  },
  "lint-staged": {
    "*.js": [
        "npm run pretty",
        "git add"
    ]
  }
}
